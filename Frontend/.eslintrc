{
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaFeatures": { "jsx": true },
    "sourceType": "module"
  },
  "root": true,
  "plugins": ["import", "react", "jsx-a11y"],

  "rules": {
    "react/prop-types": "off",
    "no-console": 1
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
